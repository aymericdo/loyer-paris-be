{
  "compilerOptions": {
    "target": "es5",
    "downlevelIteration": true,
    "module": "commonjs",
    "lib": [
      "es6"
    ],
    "baseUrl": "./src",
    "paths": {
      "@services/*": [
        "services/*"
      ],
      "@helpers/*": [
        "helpers/*"
      ],
      "@interfaces/*": [
        "interfaces/*"
      ]
    },
    "allowJs": true,
    "outDir": "./build",
    "rootDir": "./src",
    "strict": false,
    "noImplicitAny": false,
    "esModuleInterop": true,
    "resolveJsonModule": true
  }
}